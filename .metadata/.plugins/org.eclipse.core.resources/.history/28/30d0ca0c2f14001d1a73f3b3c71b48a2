import java.util.Scanner;

public class Ex3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		Scanner teclado = new Scanner(System.in);
		int [] x = new int [1000];
		
		
		for(int i = 0; i<x.length; i++) {
			System.out.println("Valor = ");
			x [i] = teclado.nextInt();
		}
	}

}
